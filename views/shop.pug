extends layout

block content
	.container
		.row
			- for(x=0;x<datashop.data.length;x++)
				.col-4.item
					figure
						img.img-fluid(src=datashop.data[x].img, alt="alt")
						figcaption 
							h2=datashop.data[x].name
							h6 $ #{datashop.data[x].money}
							p=datashop.data[x].new
						
